        <TextBlock HorizontalAlignment="Left" Margin="462,24,0,0" TextWrapping="Wrap" Text="Eastern District Soccer League" VerticalAlignment="Top" FontSize="25"/>
        <Button Content="Clubs" HorizontalAlignment="Left" Margin="30,120,0,0" VerticalAlignment="Top" Width="76"/>
        <Button Content="Teams" HorizontalAlignment="Left" Margin="32,159,0,0" VerticalAlignment="Top" Width="74"/>
